				<script type="text/javascript">
					// <![CDATA[
					$(document).ready(function() {

						$(".blococotonete").css("display","none");

						$('.tipomusica').click(function() {
							$("#fradios input:text").val("");
							$("#resultsearch").css("display","none");
							switch($(this).text()) {
								case "GÃ‰NERO":
									$('#fradios input:hidden').val(1);
									$(this).css("font-weight","bold");
									$(this).next().css("font-weight","normal");
									break;
								case "ARTISTA":
									$('#fradios input:hidden').val(2);
									$(this).css("font-weight","bold");
									$(this).prev().css("font-weight","normal");
									break;
								default:break;
							}
							return false;
						});

						$("#fradios").submit(function() {
							if(!$("#fradios input:text").val() || $("#fradios input:text").val().length<2) return false;
							else {
								var urlradio='http://www.cotonete.iol.pt/listen/cotonete_player.asp?';
								switch($("#fradios input[name='tiposearch']").val()) {
									case '2':
										urlradio+='related_artist_radio_id='+$("#fradios input[name='idmedia']").val();
										break;
									case '1':
										if($("#fradios input[name='type']").val()=='2') urlradio+='audio_type_id='+$("#fradios input[name='idmedia']").val();
										else urlradio+='audio_sub_type_id='+$("#fradios input[name='idmedia']").val();
										break;
									default:break;
								}
							}
							window.open(urlradio,'cotonete');
							return false;
						});

						$("#fradios input:text").keyup(function() {
							if($(this).val().length>=2) doSearch(escape($(this).val()));
							else $("#resultsearch").css("display","none");
							return false;
						});

					});

					jQuery.fn.jCarouselNav.defaults = ({
						btnGo: false,
						btnPausePlay: false

					})
					jQuery("#live .dir").jCarouselNav({
						goNumberCallBack: function(o){ 
							timers=false;
							var id_activo="0";
							var radio=$(".id_radio",o.div)[o.current];
							id_activo=$(radio).text();
							if(id_activo=='4752') {$(".blococotonete").css("display","block"); $().stopTime(); return false;}

							for(id in lista_radios) {
								if(lista_radios[id][4]==id_activo) {
									$(".blococotonete").css("display","none");
									$("#resultsearch").css("display","none");
									getRadioInfo(id);
									timers=true;
									//updateRadio(id_activo);
								}
							}
						},
							
							
							start: 4
							
							
							
						});
