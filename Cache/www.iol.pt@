					// <![CDATA[
					var lista_radios={
						11:['Radio Comercial','http://www.radiocomercial.iol.pt/','http://www.radiocomercial.iol.pt/quem_canta/artistas/home.aspx?id=','http://www.radiocomercial.iol.pt/quem_canta/artistas/letras_body.aspx?id=','4753','http://www.radiocomercial.iol.pt/quem_canta/artistas/album.aspx?id='],
						12:['Mix FM','http://www.mix.iol.pt/',0,0,'4776','0'],
						15:['Cidade FM','http://www.cidadefm.iol.pt/','http://www.cidadefm.iol.pt/musica/artistas/home.aspx?id=','http://www.cidadefm.iol.pt/musica/artistas/letras_body.aspx?id=','4755','http://www.cidadefm.iol.pt/musica/artistas/album.aspx?id='],
                                                17:['Star FM','http://starfm.iol.pt/','0',0,'5838','0'],
                                                30:['M80','http://www.m80.iol.pt/','http://www.m80.iol.pt/artistas/artista.aspx?id=',0,'4757','0']
                                        };

					var html_id="not_null";
					var getRadioInfo=function(id) {

						id=parseInt(id);

						var artista="";var titulo="";var album="";var id_artista=0;var id_titulo=0;
						var html_artista="";var html_titulo="";var html_capa="";var html_songid="";

						$.ajax({
							url:'/radios/CurrentRadioInfo/'+id,
							dataType:'xml',
							//data:{radio_id:id},
							type:'get',
							complete:function(xml) {
								$("#radios").find(".id_radio").each(function (i) {
									if($(this).text()==lista_radios[id][4]) {
										//html_id=$(xml.responseXML).find("DB_DALET_ITEM_CODE").text();
										//alert(html_id);
										//if($(this).parent().find(".radiovalores").html()==html_id)
										//    return true;
										//else {
										// o artista...
										artista=$(xml.responseXML).find("DB_LEAD_ARTIST_NAME").text().toLowerCase();
										if(!artista) artista=$(xml.responseXML).find("DB_DALET_ARTIST_NAME").text().toLowerCase();
										html_artista=artista;
										if($(xml.responseXML).find("DB_IS_MUSIC").text()==1 && $(xml.responseXML).find("DB_SONG_ID").text()!=0 && id!=12 && id!=30) {
											id_artista=$(xml.responseXML).find("DB_LEAD_ARTIST_ID").text();
											html_artista="<a href='"+lista_radios[id][2]+""+id_artista+"' target='_blank'>"+artista+"</a>";
										}

										// a cancao...
										titulo=$(xml.responseXML).find("DB_SONG_NAME").text().toLowerCase();
										if(!titulo) titulo=$(xml.responseXML).find("DB_DALET_TITLE_NAME").text().toLowerCase();
										html_titulo=titulo;

										if($(xml.responseXML).find("DB_IS_MUSIC").text()==1 && $(xml.responseXML).find("DB_SONG_ID").text()!=0 && id!=12 && id!=30 && id!=29) {
											id_titulo=$(xml.responseXML).find("DB_SONG_ID").text();
											html_titulo="<a href='"+lista_radios[id][3]+""+id_titulo+"' target='_blank'>"+titulo+"</a>";
										}

										if(!artista || !titulo) { html_artista=""; html_titulo="N&atilde;o est&aacute; a tocar m&uacute;sica."; }

										// ...e o album
										album=$(xml.responseXML).find("DB_ALBUM_NAME").text().toLowerCase();
										if(!album) album=$(xml.responseXML).find("DB_DALET_ALBUM_NAME").text().toLowerCase();
										if($(xml.responseXML).find("DB_ALBUM_ID").text()) {
											if(id=='16') html_album="<a href='"+lista_radios[id][5]+""+$(xml.responseXML).find("DB_LEAD_ARTIST_ID").text()+"&album_id="+$(xml.responseXML).find("DB_ALBUM_ID").text()+"' target='_blank'>"+album+"</a>";
											else if(id!='12' || id!='17' || id!='29' || id!='30') html_album="<a href='"+lista_radios[id][5]+""+$(xml.responseXML).find("DB_ALBUM_ID").text()+"' target='_blank'>"+album+"</a>";
										}
										else html_album=album;

										var capa_album=$(xml.responseXML).find("DB_ALBUM_IMAGE").text();
										var capa_radio=$(xml.responseXML).find("DB_RADIO_IMAGE").text();

										for(id_radio in lista_radios) {
											if(id_radio==id) {
												// nao tem capa
												if(id!=12) {
													var url=lista_radios[id][1]+ (capa_album ? "global_aspx/resize.aspx?img=/upload/album/"+capa_album+"&h=80&w=80" : "global_aspx/resize.aspx?img=/images/capa_indisponivel.jpg&h=80&w=80");
													html_capa="<img src='"+url+"'/>";
												}
												else {
													var url="/img/capa_indisponivel.jpg";
													html_capa="<img src='"+url+"'/>";
												}
											}
										}

										$(this).parent().find(".radiocapa").html(html_capa);
                                                                                switch (id)
                                                                                {
                                                                                case 12:
                                                                                    $(this).parent().find(".radioouvir").html('<a href="#" onclick="window.open(\'http://www.mix.iol.pt/live/live_broadcast_popup.asp\',\'radio\',\'width=790,height=380\');return false;">OUVIR</a>');
                                                                                    break;
                                                                                case 17:
                                                                                    $(this).parent().find(".radioouvir").html('<a href="#" onclick="window.open(\'http://starfm.clix.pt/\',\'radio\',\'width=790,height=380\');return false;">OUVIR</a>');
                                                                                    break;
                                                                                default:
                                                                                    $(this).parent().find(".radioouvir").html('<a href="#" onclick="window.open(\''+lista_radios[id][1]+'player/player.asp\',\'radio\',\'width=790,height=380\');return false;">OUVIR</a>');
                                                                                }
                                                                                $(this).parent().find(".radionome").html(lista_radios[id][0]);
										$(this).parent().find(".radioartista").html(html_artista);
										$(this).parent().find(".radiotitulo").html(html_titulo);
										$(this).parent().find(".radioalbum").html(html_album);
										$(this).parent().find(".radiovalores").html(html_id);


										//}
									}
								});
							}
						});

					}


					var timers=false;
					var updateRadio=function(id_activo) {
						if(timers) $().stopTime();
						$().everyTime(20000,'radios',function() {
							for(id in lista_radios) {
								if(lista_radios[id][4]==id_activo) {
									getRadioInfo(id);
								}
							}
						});
					}

					var doSearch=function(search) {
						$("#resultsearch").css("display","block");
						var results="<div style='background-color: white; color: black; text-align: left; position: absolute; min-width: 176px; padding-left: 5px; clear: both; width: 176px;'>\n";
						var tipo=$("#fradios input[name='tiposearch']").val();
						if(tipo=='2') var urlradiosearch='/radios/SearchArtist/'+search;
						else var urlradiosearch='/radios/SearchAudio/5/'+search;
						$.ajax({
							url:urlradiosearch,
							dataType:'xml',
							//data:{fk_site_id:'5',search_string:search},
							type:'get',
							beforeSend:function() {
								$("#resultsearch").html("<div style='background-color: white; color: black; text-align: left; position: absolute; min-width: 176px; padding-left: 5px; clear: both; width: 176px;'>\n<img src='img/loading.gif'/></div>");
							},
							success:function(xml) {
								if($(xml).find("nothing_to_return").text()) $("#resultsearch").css("display","none");
								else {
									$(xml).find("Result").each(function(i,val) {
										if(tipo=='2') {
											results+="<span onclick='putThis(this.innerHTML,"+$(this).find('SEARCH_ID').text()+",0)' onmouseout='this.style.cssText=\"color:black;background-color:white\";' onmouseover='this.style.cssText=\"cursor:pointer;color:white;background-color:black\";'>";
											results+=$(this).find("SEARCH_NAME").text();
										}
										else {
											if($(this).find("AUDIO_SUB_TYPE_NAME").text()) {
												results+="<span onclick='putThis(this.innerHTML,"+$(this).find('AUDIO_SUB_TYPE_ID').text()+",1)' onmouseout='this.style.cssText=\"color:black;background-color:white\";' onmouseover='this.style.cssText=\"cursor:pointer;color:white;background-color:black\";'>";
												results+=$(this).find("AUDIO_SUB_TYPE_NAME").text();
											}
											else {
												results+="<span onclick='putThis(this.innerHTML,"+$(this).find('AUDIO_TYPE_ID').text()+",2)' onmouseout='this.style.cssText=\"color:black;background-color:white\";' onmouseover='this.style.cssText=\"cursor:pointer;color:white;background-color:black\";'>";
												results+=$(this).find("AUDIO_TYPE_NAME").text();
											}
										}
										results+="</span>\n<br/>\n";
									});
								}
								results+="</div>\n";
								$("#resultsearch").html(results);
							}
						});
					}

					var putThis=function(nome,id,type) {
						$(document).ready(function() {
							nome=nome.replace(/&amp;/i,"&");
							$("#fradios input[name='idmedia']").val(id);
							$("#fradios input[name='type']").val(type);
							$("#fradios input[name='nome']").val(nome);
							$("#resultsearch").css("display","none");
						});
					}
