					<script type="text/javascript">
						// <![CDATA[
						// separadores lazer
						var firstlazer=true;
						var $manchete2Tabs = jQuery("#separadoreslazer").tabs({
							selected: (typeof(mancheteSeleccionada2) != 'undefined' ? mancheteSeleccionada2 : 0),
							show: function(event, ui){
                            


								jQuery(ui.panel).html(jQuery(ui.panel).html().replace(/--/g, "").replace(/!/g, "")); // retira o !-- e -- das imagens da tab actual
								if(ui.tab.toString().indexOf("#tabfotos") > 0){
									var sizeX=(document.documentElement.clientWidth ? document.documentElement.clientWidth : document.body.clientWidth)-50;
									var sizeY=(document.documentElement.clientHeight ? document.documentElement.clientHeight : document.body.clientHeight)-35;
									jQuery("#mfotos #mainfotolink").ultraLightBox({width:sizeX,height:sizeY});
									jQuery(".descricao #mainfotolinkmax").ultraLightBox({width:sizeX,height:sizeY});
									jQuery(".navegacao .consolaGaleriaFotos").ultraLightBox({width:sizeX,height:sizeY});
									jQuery(".mlazer .consolaGaleriaFotos").ultraLightBox({width:sizeX,height:sizeY});
								}
							}
						}).hover(function () {
							$manchete2Tabs.tabs('rotate', null);
						});
