<script type='text/javascript'>


//var currenttime = '<!--#config timefmt='%B %d, %Y %H:%M:%S'--><!--#echo var='DATE_LOCAL' -->' //SSI method of getting server date
var currenttime = 'December 15, 2011 00:44:06' //PHP method of getting server date

///////////Stop editting here/////////////////////////////////

var montharray=new Array('January','February','March','April','May','June','July','August','September','October','November','December')
var serverdate=new Date(currenttime)

function padlength(what){
var output=(what.toString().length==1)? '0'+what : what
return output
}

function displaytime(){
serverdate.setSeconds(serverdate.getSeconds()+1)
var datestring=montharray[serverdate.getMonth()]+' '+padlength(serverdate.getDate())+', '+serverdate.getFullYear()
var test_date
if(serverdate.getHours()<12){
	test_date = serverdate.getHours()
	test_time = 'AM'
	}
else if(serverdate.getHours()>12){
	test_date = serverdate.getHours()-13
	test_time = 'PM'
	}
else{
	test_date = serverdate.getHours()
	test_time = 'PM'	
}
var timestring=padlength(test_date)+':'+padlength(serverdate.getMinutes())+':'+padlength(serverdate.getSeconds())+' '+padlength(test_time)
document.getElementById('servertime').innerHTML=datestring+'<br /><br />'+timestring
}

window.onload=function(){
setInterval('displaytime()', 1000)
}

</script>
<p style='text-align: center;'> <span id='servertime'></span></p>
</center></div></li>
